  "scripts": {
    "dev": "vite",
    "build": "vite build --mode staging && tsc",
    "build:react": "react-scripts build --profile",
    "clean": "rm -rf build && find ./src -name '*.js' -type f | xargs rm -f",
    "preview": "vite build --mode staging && vite preview",
    "deploy:staging": "vite build --mode staging && firebase deploy --only hosting --project=staging",
