{
  "name": "firecms",
  "version": "3.0.0-canary.33",
  "description": "A CLI to perform FireCMS operations",
  "packageManager": "yarn@4.1.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.es.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "source": "src/index.ts",
  "bin": "bin/firecms.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf templates/template_cloud/node_modules && rm -rf templates/template_cloud/yarn.lock && vite build && tsc --emitDeclarationOnly -p tsconfig.json",
    "dev": "ts-node src/test.ts",
    "clean": "rm -rf dist && find ./src -name '*.js' -type f | xargs rm -f"
  },
  "keywords": [
    "cli",
    "create-firecms-app",
    "firecms",
    "firebase",
    "firestore",
    "cms",
    "react",
    "typescript"
  ],
  "author": "firecms.co",
  "license": "MIT",
  "dependencies": {
    "firecms": "^3.0.0-canary.32"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "vite": "^5.2.3"
  },
  "files": [
    "bin/",
    "dist/",
    "src/",
    "templates/"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "exclude": [
    "node_modules"
  ],
  "gitHead": "e26534245ded1f4523f180233cfda316c061fa8c"
}
