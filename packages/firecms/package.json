{
  "name": "firecms",
  "type": "module",
  "version": "3.0.0-canary.7",
  "publishConfig": {
    "access": "public"
  },
  "bin": "./bin/firecms.mjs",
  "keywords": [
    "firebase",
    "cms",
    "admin",
    "admin panel",
    "firebase panel",
    "firestore",
    "headless",
    "headless cms",
    "content manager"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./tailwind.config.js": "./tailwind.config.js"
  },
  "packageManager": "yarn@4.1.0",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "dependencies": {
    "@firecms/cli": "^3.0.0-canary.7",
    "@firecms/collection_editor": "^3.0.0-canary.7",
    "@firecms/core": "^3.0.0-canary.7",
    "@firecms/data_enhancement": "^3.0.0-canary.7",
    "@firecms/data_import_export": "^3.0.0-canary.7",
    "@firecms/firebase": "^3.0.0-canary.7",
    "@firecms/formex": "^3.0.0-canary.7",
    "@firecms/schema_inference": "^3.0.0-canary.7",
    "react-router": "^6.22.0",
    "react-router-dom": "^6.22.0"
  },
  "peerDependencies": {
    "firebase": "^10.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.54",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.1"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly -p tsconfig.prod.json",
    "clean": "rm -rf dist && find ./src -name '*.js' -type f | xargs rm -f"
  },
  "files": [
    "dist",
    "src",
    "tailwind.config.js",
    "bin"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "gitHead": "04fbd0c86fa6134b2ef84d40dc8ef36af7a28a8e"
}
