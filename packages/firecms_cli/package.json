{
  "name": "@firecms/cli",
  "version": "3.0.0-canary.12",
  "description": "A CLI to perform FireCMS operations",
  "packageManager": "yarn@4.1.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "source": "src/index.ts",
  "bin": {
    "firecms": "bin/firecms"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf templates/template_v3/node_modules && rm -rf templates/template_v3/yarn.lock && run-s build:esm build:cjs",
    "dev": "ts-node src/index.ts",
    "clean": "rm -rf dist && find ./src -name '*.js' -type f | xargs rm -f"
  },
  "keywords": [
    "cli",
    "create-firecms-app",
    "firecms",
    "firebase",
    "firestore",
    "cms",
    "react",
    "typescript"
  ],
  "author": "firecms.co",
  "license": "MIT",
  "dependencies": {
    "arg": "^5.0.2",
    "axios": "^0.28.0",
    "chalk": "^4.1.2",
    "execa": "^4.1.0",
    "form-data": "^4.0.0",
    "googleapis": "^134.0.0",
    "inquirer": "^7.3.3",
    "json5": "^2.2.3",
    "listr": "^0.14.3",
    "ncp": "^2.0.0",
    "nocache": "^4.0.0",
    "open": "^10.1.0",
    "ora": "^8.0.1",
    "ws": "^8.16.0",
    "zip-folder": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "files": [
    "bin/",
    "build/",
    "src/",
    "templates/"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "exclude": [
    "node_modules",
    "template/node_modules"
  ],
  "gitHead": "b1353723c0d01e56aa29a406aa7d6689d0bbce1c"
}
