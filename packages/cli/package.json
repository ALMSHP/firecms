    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "cli",
    "create-firecms-app",
    "firecms",
    "firebase",
    "firestore",
    "cms",
    "react",
    "typescript"
  ],
  "author": "firecms.co",
  "license": "MIT",
  "dependencies": {
    "firecms": "^3.0.0-canary.35"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "vite": "^5.2.3"
  },
  "files": [
    "bin/",
    "dist/",
    "src/",
    "templates/"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "exclude": [
    "node_modules"
