{
  "name": "@firecms/cli",
  "version": "3.0.0-canary.35",
  "description": "A CLI to perform FireCMS operations",
  "packageManager": "yarn@4.1.0",
  "type": "commonjs",
  "bin": {
    "firecms": "bin/firecms.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "echo \"@firecms/cli: no need to build\" && exit 0",
