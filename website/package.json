{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "dev": "yarn copyChangelog && docusaurus start",
    "build": "REACT_GENERATE_API=true && NODE_OPTIONS=--max-old-space-size=24048 && yarn copyChangelog && docusaurus build",
    "preview": "yarn build && docusaurus serve",
    "genAPI": "typedoc --plugin typedoc-plugin-markdown --hideGenerator --hideInPageTOC --out docs/api",
    "copyChangelog": "echo '---' > ./docs/CHANGELOG.md && echo 'id: changelog' >> ./docs/CHANGELOG.md && echo 'title: Changelog' >> ./docs/CHANGELOG.md && echo '---' >> ./docs/CHANGELOG.md && cat ../CHANGELOG.md >> ./docs/CHANGELOG.md ",
    "swizzle": "docusaurus swizzle --danger",
    "deploy": "yarn build && firebase deploy --only hosting",
    "serve": "docusaurus serve",
    "clear": "docusaurus clear"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@docsearch/js": "3.6.1",
    "@docusaurus/core": "^3.4.0",
    "@docusaurus/plugin-google-gtag": "^3.4.0",
    "@docusaurus/plugin-sitemap": "^3.4.0",
    "@docusaurus/preset-classic": "^3.4.0",
    "@docusaurus/theme-search-algolia": "^3.4.0",
    "@firecms/collection_editor": "link:../packages/collection_editor",
    "@firecms/core": "link:../packages/firecms_core",
    "@firecms/data_enhancement": "link:../packages/data_enhancement",
    "@firecms/data_export": "link:../packages/data_export",
    "@firecms/data_import": "link:../packages/data_import",
    "@firecms/firebase": "link:../packages/firebase_firecms",
    "@firecms/formex": "link:../packages/formex",
    "@firecms/neat": "0.2.2",
    "@firecms/schema_inference": "link:../packages/schema_inference",
    "@firecms/ui": "link:../packages/ui",
    "@mdx-js/react": "^3.0.1",
    "aos": "^2.3.4",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "postprocessing": "^6.36.0",
    "prism-react-renderer": "^2.3.1",
    "raw-loader": "^4.0.2",
    "react": "link:../node_modules/react",
    "react-dom": "link:../node_modules/react-dom",
    "react-fast-marquee": "^1.6.5",
    "react-responsive-masonry": "^2.3.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-transition-group": "^4.4.5",
    "react-type-animation": "^2.1.2",
    "sass": "^1.77.8",
    "tailwindcss": "^3.4.7",
    "three": "^0.149.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.4.0",
    "@docusaurus/types": "^3.4.0",
    "@firecms/cloud": "link:../packages/firecms_cloud",
    "@tsconfig/docusaurus": "^2.0.3",
    "@types/react": "^18.3.3",
    "@types/react-helmet": "^6.1.11",
    "@types/react-router-dom": "^5.3.3",
    "docusaurus-plugin-sass": "^0.2.5",
    "docusaurus-plugin-typedoc": "^1.0.4",
    "docusaurus-plugin-typedoc-api": "^4.2.1",
    "docusaurus-tailwindcss-loader": "file:plugins/docusaurus-tailwindcss-loader",
    "esbuild-loader": "^4.2.2",
    "fontaine": "^0.5.0",
    "openai": "^4.55.0",
    "postcss": "^8.4.41",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^9.6.0",
    "typedoc": "^0.26.5",
    "typedoc-plugin-markdown": "^4.2.3",
    "typescript": "^5.5.4"
  }
}
